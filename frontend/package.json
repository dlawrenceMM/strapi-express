{
  "name": "strapi-frontend-example",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start-prod": "node --trace-warnings app.js",
    "start-dev": "nodemon --watch --trace-warnings app.js",
    "webpack-prod": "yarn webpack --progress --mode=production",
    "webpack-dev": "yarn webpack --watch --progress --mode=development",
    "build": "yarn webpack-prod",
    "dev": "concurrently \"yarn webpack-dev\" \"yarn start-dev\""
  },
  "dependencies": {
    "body-parser": "1.13.2",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "1.3.5",
    "cookie-session": "^1.2.0",
    "debug": "2.2.0",
    "ejs": "^3.1.9",
    "express": "4.13.1",
    "express-session": "^1.13.0",
    "jade": "1.11.0",
    "lodash": "^4.1.0",
    "marked": "^0.3.5",
    "morgan": "1.6.1",
    "platformsh-config": "^2.4.1",
    "react-redux": "^8.0.5",
    "request": "^2.69.0",
    "serve-favicon": "2.3.0",
    "superagent": "^1.7.2",
    "web-vitals": "^3.3.1",
    "webpack": "^5.81.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "cache-loader": "^4.1.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss-loader": "^7.3.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "terser-webpack-plugin": "^5.3.7",
    "webpack-cli": "^5.0.2"
  }
}
